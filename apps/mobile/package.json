{
	"name": "mobile",
	"main": "index.js",
	"version": "0.0.0",
	"scripts": {
		"start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start",
		"android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
		"ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
		"prebuild": "expo prebuild",
		"prebuild:clean": "expo prebuild --clean",
		"style:check": "prettier --check .",
		"style:fix": "prettier --write .",
		"types:check": "tsc --noEmit --allowImportingTsExtensions",
		"lint": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"code:check": "npm run types:check && npm run style:check && npm run lint",
		"clean": "react-native clean",
		"clean:all": "npm run clean -- --include npm,metro,watchman,android,cocoapods",
		"build:apk": "eas build --profile preview --platform android"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.0.2",
