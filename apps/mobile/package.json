{
	"name": "mobile",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"node": "20.x.x",
		"npm": "10.x.x"
	},
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint:js": "npx eslint \"src/**/*.{ts,tsx}\"",
		"lint:type": "npx tsc --noEmit",
		"start:dev": "react-native start --reset-cache",
		"install:pods": "NO_FLIPPER=1 bundle exec pod install --project-directory=ios",
		"install:gems": "bundle install",
		"install:all": "npm install && npm run install:gems && npm run install:pods"
	},
	"dependencies": {
		"@hookform/resolvers": "3.9.0",
		"@react-navigation/native": "6.1.18",
